{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["gru1"],
  "crons": [
    {
      "path": "/api/cron/auto-reservations",
      "schedule": "0 0 * * *"
    }
  ]
}
